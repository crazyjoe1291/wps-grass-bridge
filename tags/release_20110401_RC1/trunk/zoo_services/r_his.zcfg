[r_his]
Title = Generates red, green and blue raster map layers combining hue, intensity and saturation HIS values from user-specified input raster map layers.
Abstract = http://grass.osgeo.org/grass70/manuals/html70_user/r.his.html
processVersion = 2
storeSupported = true 
statusSupported = true
serviceProvider = r_his
serviceType = Python
<DataInputs>
  [h_map]
  Title = Name of layer to be used for HUE
  Abstract =
  minOccurs = 1
  maxOccurs = 1
  <ComplexData>
    <Default>
      mimeType = image/tiff
    </Default>
    <Supported>
      mimeType = image/tiff
    </Supported>
    <Supported>
      mimeType = image/geotiff
    </Supported>
    <Supported>
      mimeType = application/geotiff
    </Supported>
    <Supported>
      mimeType = application/x-geotiff
    </Supported>
    <Supported>
      mimeType = image/png
    </Supported>
    <Supported>
      mimeType = image/gif
    </Supported>
    <Supported>
      mimeType = image/jpeg
    </Supported>
    <Supported>
      mimeType = application/x-erdas-hfa
    </Supported>
    <Supported>
      mimeType = application/netcdf
    </Supported>
    <Supported>
      mimeType = application/x-netcdf
    </Supported>
  </ComplexData>
  [i_map]
  Title = Name of layer to be used for INTENSITY
  Abstract =
  minOccurs = 0
  maxOccurs = 1
  <ComplexData>
    <Default>
      mimeType = image/tiff
    </Default>
    <Supported>
